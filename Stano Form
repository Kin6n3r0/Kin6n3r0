<p><script>
jQuery( document ).ready(function() {
	jQuery('#golf_ind_qty').change(function(){
		var qty = parseInt(jQuery('#golf_ind_qty').val());
		jQuery('#UMline1qty').val(qty)
		jQuery('#golf_ind').text("$" + qty * 65);
		updateTotal();
	});
	jQuery('#prize_ind_qty').change(function(){
		var qty = parseInt(jQuery('#prize_ind_qty').val());
		jQuery('#UMline3qty').val(qty)
		jQuery('#prize_ind').text("$" + qty * 10);
		updateTotal();
	});
	jQuery('#spon_prem_qty').change(function(){
		var qty = parseInt(jQuery('#spon_prem_qty').val());
		jQuery('#UMline4qty').val(qty)
		jQuery('#spon_prem').text("$" + qty * 1000);
		updateTotal();
	});
	jQuery('#spon_golf_qty').change(function(){
		var qty = parseInt(jQuery('#spon_golf_qty').val());
		jQuery('#UMline5qty').val(qty)
		jQuery('#spon_golf').text("$" + qty * 300);
		updateTotal();
	});
	jQuery('#spon_hole_qty').change(function(){
		var qty = parseInt(jQuery('#spon_hole_qty').val());
		jQuery('#UMline6qty').val(qty)
		jQuery('#spon_hole').text("$" + qty * 100);
		updateTotal();
	});
	jQuery('#year').change(function(){
		var year = jQuery('#year').val().toString();
		var month = jQuery('#month').val().toString();
		jQuery('#UMexpir').val(month+year);
console.log(month+year);
	});
	jQuery('#month').change(function(){
		var year = jQuery('#year').val().toString();
		var month = jQuery('#month').val().toString();
		jQuery('#UMexpir').val(month+year);
console.log(month+year);
	});
});
function updateTotal() {
var golf_ind_qty = parseInt(jQuery('#golf_ind_qty').val())*65;
var prize_ind_qty = parseInt(jQuery('#prize_ind_qty').val())*10;
var spon_prem_qty = parseInt(jQuery('#spon_prem_qty').val())*1000;
var spon_golf_qty = parseInt(jQuery('#spon_golf_qty').val())*300;
var spon_hole_qty = parseInt(jQuery('#spon_hole_qty').val())*100;
var total = golf_ind_qty + prize_ind_qty + spon_prem_qty + spon_golf_qty + spon_hole_qty;
jQuery('#total').text("$" + total);
jQuery('#UMamount').val(total);
};
</script></p>
